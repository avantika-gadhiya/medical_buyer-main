<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorWhite"
    tools:context=".activities.ViewOfferActivity">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="@dimen/dp_16" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_end"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="@dimen/dp_16" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/con_toolbar"
        android:layout_width="0dp"
        android:layout_height="?attr/actionBarSize"
        android:background="@color/colorWhite"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/img_back"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:padding="@dimen/dp_15"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/back_arrow" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/circularstd_medium"
            android:gravity="center"
            android:text="@string/view_offer_"
            android:textAllCaps="true"
            android:textColor="@color/black_txt"
            android:textSize="@dimen/sp_16"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <View
        android:id="@+id/view_1"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="@color/bg_color"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/con_toolbar" />

    <androidx.core.widget.NestedScrollView
        android:id="@+id/nested_location_price"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:clipToPadding="true"
        android:fillViewport="true"
        android:fitsSystemWindows="true"
        app:layout_constraintBottom_toTopOf="@+id/constraint_bottom"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_1">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/bg_color">

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline_st"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_begin="@dimen/dp_16" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline_en"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_end="@dimen/dp_16" />

            <androidx.cardview.widget.CardView
                android:id="@+id/constraint1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_16"
                android:layout_marginBottom="@dimen/dp_16"
                app:cardCornerRadius="@dimen/dp_5"
                app:layout_constraintBottom_toTopOf="@+id/constraint2"
                app:layout_constraintEnd_toStartOf="@+id/guideline_en"
                app:layout_constraintStart_toEndOf="@+id/guideline_st"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_order_id"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/circularstd_book"
                        android:text="ID: 12345 67890"
                        android:textColor="@color/black_txt"
                        android:textSize="@dimen/sp_18"
                        app:layout_constraintBaseline_toBaselineOf="@+id/txt_status1"
                        app:layout_constraintStart_toStartOf="parent" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_datentime"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_5"
                        android:fontFamily="@font/circularstd_medium"
                        android:text="5 JUN, 2019 2:35 PM"
                        android:textColor="@color/grey_txt"
                        android:textSize="@dimen/sp_14"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/txt_order_id" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_status"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_marginTop="@dimen/dp_10"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:background="@drawable/custom_status"
                        android:backgroundTint="@color/bg_color_txt"
                        android:fontFamily="@font/circularstd_book"
                        android:text="@string/offer_made"
                        android:textColor="@color/colorWhite"
                        android:textSize="@dimen/sp_12"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_status1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="10dp"
                        android:background="@drawable/custom_status"
                        android:backgroundTint="@color/grey_txt"
                        android:fontFamily="@font/circularstd_book"
                        android:text="@string/active_status"
                        android:textColor="@color/colorWhite"
                        android:textSize="@dimen/sp_12"
                        app:layout_constraintBaseline_toBaselineOf="@+id/txt_status"
                        app:layout_constraintEnd_toStartOf="@+id/txt_status" />

                    <View
                        android:id="@+id/view"
                        android:layout_width="0dp"
                        android:layout_height="1dp"
                        android:layout_marginTop="16dp"
                        android:background="@color/bg_color"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/txt_datentime" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_time_elpsed"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/dp_10"
                        android:fontFamily="@font/circularstd_bookitalic"
                        android:text="Time Elapsed: 16h 25m 40s"
                        android:textColor="@color/green_txt"
                        android:textSize="@dimen/sp_14"
                        app:layout_constraintBaseline_toBaselineOf="@+id/txt_view_order"
                        app:layout_constraintStart_toStartOf="parent" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_view_order"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/dp_10"
                        android:fontFamily="@font/circularstd_medium"
                        android:text="@string/view_order"
                        android:textColor="@color/black_txt"
                        android:textSize="@dimen/sp_14"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="1.0"
                        app:layout_constraintStart_toEndOf="@+id/txt_time_elpsed"
                        app:layout_constraintTop_toBottomOf="@+id/view" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/constraint2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dp_16"
                app:cardCornerRadius="@dimen/dp_5"
                app:layout_constraintBottom_toTopOf="@+id/constraint3"
                app:layout_constraintEnd_toStartOf="@+id/guideline_en"
                app:layout_constraintStart_toEndOf="@+id/guideline_st"
                app:layout_constraintTop_toBottomOf="@+id/constraint1">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/textView58"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_16"
                        android:fontFamily="@font/circularstd_medium"
                        android:text="@string/order_offer"
                        android:textAllCaps="true"
                        android:textColor="@color/grey_txt"
                        android:textSize="@dimen/sp_13"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/textView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_16"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:fontFamily="@font/circularstd_medium"
                        android:text="@string/mrp"
                        android:textAllCaps="true"
                        android:textColor="@color/grey_txt"
                        android:textSize="@dimen/sp_13"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintTop_toTopOf="parent" />

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/recycl_order_offer"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dp_16"
                        android:padding="@dimen/dp_10"
                        android:visibility="visible"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView58" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_view_all_produts"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_8"
                        android:fontFamily="@font/circularstd_medium"
                        android:gravity="center"
                        android:padding="@dimen/dp_12"
                        android:text="@string/view_all_products"
                        android:textColor="@color/colorAccent"
                        android:textSize="@dimen/sp_15"
                        android:visibility="visible"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/recycl_order_offer" />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/constraint3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dp_16"
                app:cardCornerRadius="@dimen/dp_5"
                app:layout_constraintBottom_toTopOf="@+id/constraint4"
                app:layout_constraintEnd_toStartOf="@+id/guideline_en"
                app:layout_constraintStart_toEndOf="@+id/guideline_st"
                app:layout_constraintTop_toBottomOf="@+id/constraint2">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/textView59"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_16"
                        android:fontFamily="@font/circularstd_medium"
                        android:text="@string/buying_prefrnc"
                        android:textAllCaps="true"
                        android:textColor="@color/grey_txt"
                        android:textSize="@dimen/sp_13"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_buying_pref"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_10"
                        android:layout_marginBottom="@dimen/dp_16"
                        android:fontFamily="@font/circularstd_medium"
                        android:text="Only Prescription Brand"
                        android:textColor="@color/red_txt"
                        android:textSize="@dimen/sp_16"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView59" />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/constraint4"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dp_16"
                app:cardCornerRadius="@dimen/dp_5"
                app:layout_constraintBottom_toTopOf="@+id/constraint5"
                app:layout_constraintEnd_toStartOf="@+id/guideline_en"
                app:layout_constraintStart_toEndOf="@+id/guideline_st"
                app:layout_constraintTop_toBottomOf="@+id/constraint3">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/textView9"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_marginTop="16dp"
                        android:fontFamily="@font/circularstd_medium"
                        android:text="@string/delivery_prefrnc"
                        android:textAllCaps="true"
                        android:textColor="@color/grey_txt"
                        android:textSize="@dimen/sp_13"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_del_pref"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_10"
                        android:fontFamily="@font/circularstd_medium"
                        android:text="Only Store Pickup"
                        android:textColor="@color/black_txt"
                        android:textSize="@dimen/sp_16"
                        app:layout_constraintBottom_toTopOf="@+id/txt_del_add"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView9"
                        app:layout_goneMarginBottom="@dimen/dp_16" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_del_add"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:fontFamily="@font/circularstd_medium"
                        android:textColor="@color/grey_txt"
                        android:textSize="@dimen/sp_14"
                        android:visibility="visible"
                        app:layout_constraintBottom_toTopOf="@+id/txt_geo_location"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/txt_del_pref"
                        app:layout_goneMarginBottom="@dimen/dp_16" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_geo_location"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_5"
                        android:layout_marginBottom="@dimen/dp_16"
                        android:fontFamily="@font/circularstd_book"
                        android:textColor="@color/blue_clr"
                        android:drawableTint="@color/blue_clr"
                        android:textSize="@dimen/sp_14"
                        android:visibility="gone"
                        app:drawableTint="@color/blue_clr"
                        android:drawableStart="@drawable/google_pin"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/txt_del_add" />
                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/constraint5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dp_16"
                app:cardCornerRadius="@dimen/dp_5"
                app:layout_constraintBottom_toTopOf="@+id/constraint6"
                app:layout_constraintEnd_toStartOf="@+id/guideline_en"
                app:layout_constraintStart_toEndOf="@+id/guideline_st"
                app:layout_constraintTop_toBottomOf="@+id/constraint4">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">


                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_delivery_date"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:layout_marginBottom="16dp"
                        android:fontFamily="@font/circularstd_book"
                        android:text="10 july, 2019"
                        android:textColor="@color/black_txt"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_delivery_time"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:fontFamily="@font/circularstd_book"
                        android:text="From 11 am to 1 pm"
                        android:textColor="@color/black_txt"
                        android:layout_marginStart="@dimen/dp_20"
                        android:visibility="gone"
                        app:layout_constraintBaseline_toBaselineOf="@+id/txt_delivery_date"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/textView76"  />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/textView76"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:fontFamily="@font/circularstd_medium"
                        android:text="@string/delivery_date"
                        android:textColor="@color/black_txt"
                        android:textSize="@dimen/sp_16"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintBottom_toTopOf="@+id/txt_delivery_date"
                        app:layout_constraintStart_toStartOf="parent" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/textView77"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:fontFamily="@font/circularstd_medium"
                        android:text="@string/delivery_time"
                        android:textColor="@color/black_txt"
                        android:textSize="@dimen/sp_16"
                        android:visibility="gone"
                        android:layout_marginStart="@dimen/dp_20"
                        app:layout_constraintBaseline_toBaselineOf="@+id/textView76"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/textView76" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/textView78"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_16"
                        android:layout_marginBottom="@dimen/dp_16"
                        android:fontFamily="@font/circularstd_medium"
                        android:text="@string/delivery_date_time"
                        android:textAllCaps="true"
                        android:textColor="@color/grey_txt"
                        android:textSize="@dimen/sp_13"
                        app:layout_constraintBottom_toTopOf="@+id/textView76"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/constraint6"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dp_16"
                app:cardCornerRadius="@dimen/dp_5"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/guideline_en"
                app:layout_constraintStart_toEndOf="@+id/guideline_st"
                app:layout_constraintTop_toBottomOf="@+id/constraint5">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/textView90"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_marginTop="16dp"
                        android:fontFamily="@font/circularstd_medium"
                        android:text="@string/order_amount"
                        android:textAllCaps="true"
                        android:textColor="@color/grey_txt"
                        android:textSize="@dimen/sp_13"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/textView79"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_marginTop="@dimen/dp_16"
                        android:fontFamily="@font/circularstd_book"
                        android:text="@string/bill_amount"
                        android:textColor="@color/black_txt"
                        android:textSize="@dimen/sp_16"
                        app:layout_constraintEnd_toStartOf="@+id/textView80"
                        app:layout_constraintHorizontal_bias="0.0"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/textView90" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/textView80"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/dp_5"
                        android:fontFamily="@font/circularstd_book"
                        android:text="@string/mrp"
                        android:visibility="gone"
                        android:textColor="@color/grey_txt"
                        app:layout_constraintBaseline_toBaselineOf="@+id/textView79"
                        app:layout_constraintEnd_toStartOf="@+id/txt_bill_amount" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/txt_bill_amount"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/dp_10"
                        android:fontFamily="@font/circularstd_book"
                        android:text="₹ 6589.00"
                        android:textColor="@color/black_txt"
                        app:layout_constraintBaseline_toBaselineOf="@+id/textView80"
                        app:layout_constraintEnd_toEndOf="parent" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/textView82"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_10"
                        android:fontFamily="@font/circularstd_book"
                        android:text="@string/txt_inclusiv_taxes"
                        android:textColor="@color/grey_txt"
                        android:textSize="@dimen/sp_10"
                        app:layout_constraintStart_toStartOf="parent"
